{
  "name": "@co-code/admin-server",
  "version": "0.1.0",
  "type": "module",
  "description": "Admin server with web UI - agent creation, CT scan, and wellbeing dashboard",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:server && npm run build:web",
    "build:server": "tsc",
    "build:web": "vite build --config web/vite.config.ts",
    "dev": "tsx watch src/index.ts",
    "dev:web": "vite --config web/vite.config.ts",
    "start": "node dist/index.js",
    "test": "vitest run --passWithNoTests",
    "lint": "eslint src",
    "db:migrate": "node dist/db/migrate.js"
  },
  "dependencies": {
    "@co-code/shared": "*",
    "@fastify/cors": "^10.0.0",
    "@fastify/jwt": "^9.0.0",
    "@fastify/static": "^8.0.0",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.4.5",
    "fastify": "^5.2.0",
    "pg": "^8.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.3",
    "uuid": "^11.0.0",
    "zod": "^3.24.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "jsdom": "^24.1.3",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vite": "^5.4.0",
    "vitest": "^2.1.0"
  }
}
